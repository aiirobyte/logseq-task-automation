{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": [
      "warn",
      {
        "allow": ["no-console"]
      }
    ],
    "no-await-in-loop": "off",
    "no-restricted-globals": [1, "parent"],
    "linebreak-style": ["error", "windows"],
    "quotes": ["error", "double"]
  },
  "globals": {
    "logseq": "readonly"
  }
}
